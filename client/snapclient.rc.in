id ICON "@CMAKE_SOURCE_DIR@/platform/windows/icon.ico"
1 VERSIONINFO
FILEVERSION @PROJECT_VERSION_MAJOR@,@PROJECT_VERSION_MINOR@,@PROJECT_VERSION_PATCH@,@PROJECT_VERSION_TWEAK@
PRODUCTVERSION @PROJECT_VERSION_MAJOR@,@PROJECT_VERSION_MINOR@,@PROJECT_VERSION_PATCH@,@PROJECT_VERSION_TWEAK@
BEGIN
  BLOCK "StringFileInfo"
	BEGIN
	  BLOCK "040904E4"
		BEGIN
		  VALUE "CompanyName", "@CPACK_PACKAGE_VENDOR@"
			VALUE "FileDescription", "@CPACK_PACKAGE_DESCRIPTION_SUMMARY@"
			VALUE "FileVersion", "@PROJECT_VERSION@"
			VALUE "InternalName", "@PROJECT_NAME@"
			VALUE "LegalCopyright", "@CPACK_PACKAGE_CONTACT@"
			VALUE "OriginalFilename", "@snapclient.exe@"
			VALUE "ProductName", "snapclient"
			VALUE "ProductVersion", "@PROJECT_VERSION@"
		END
	END

  BLOCK "VarFileInfo"
	BEGIN
	  VALUE "Translation", 0x409, 1252
	END
END